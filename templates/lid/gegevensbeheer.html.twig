{% extends 'base.html.twig' %}
    {% block title %}registreren{% endblock %}
    {% block stylesheets %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ asset('css/login.css') }}">
    {% endblock %}
    {% block body %}
        <div class="container justify-content-center">
            <h3>{{ app.user.lastName }} {{ app.user.preprovision }} {{ app.user.lastName }}</h3>
            <table>
                <tr>
                    <td><p>GeboorteDatum</p></td>
                    <td colspan="2" class="tekst">{{ app.user.getDateofbirth2() }}</td>
                </tr>
                <tr>
                    <td><p>Gebruikersnaam</p></td>
                    <td colspan="2" class="tekst">{{ app.user.username }}</td>
                </tr>
                <tr>
                    <td><p>Man/vrouw</p></td>
                    <td colspan="2" class="tekst">{{ app.user.gender }}</td>
                </tr>
                <tr>
                    <td><p>adres:</p></td>
                    <td colspan="2" class="tekst"></td>
                </tr>
                <tr>
                    <td><p>postcode:</p></td>
                    <td colspan="2" class="tekst"></td>
                </tr>
                <tr>
                    <td><p>Woonplaats:</p></td>
                    <td colspan="2" class="tekst"></td>
                </tr>
                <tr>
                    <td><p>Email:</p></td>
                    <td colspan="2" class="tekst"></td>
                </tr>
            </table>
        </div>
    {% endblock %}
